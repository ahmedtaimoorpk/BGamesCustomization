/**
 * Web page structure.
 */
{template .start}
    {call .messages /}
    <table id="header" width="100%">
        <tr>
            <td valign="bottom">
                <h1>
                    {call BlocklyGames.soy.titleSpan}
                        {param appName}
                            {msg meaning="Games.puzzle" desc="IBID"}
                                Puzzle
                            {/msg}
                        {/param}


                    {/call}
                    {call BlocklyGames.soy.levelLinks}
                        {param level: $ij.level /}
                        {param maxLevel: $ij.maxLevel /}
                        {param lang: $ij.lang /}
                        {param suffix}&skin={$ij.skin}{/param}
                    {/call}
                </h1>
            </td>
            <td class="farSide">
                <select id="languageMenu"></select>
                &nbsp;
                <button id="helpButton">
                    {msg meaning="Games.help" desc="IBID"}Help{/msg}
                </button>
                &nbsp;
                <button id="checkButton" class="primary">
                    {msg meaning="Puzzle.checkAnswers"
                            desc="A label on a button the user can press to check his/her answers."}
                        Check Answers
                    {/msg}
                </button>
            </td>
        </tr>
    </table>

    <div id="blockly"></div>
    <div id="images" style="min-width: 100%; position: absolute; left: 90%; top: 85%;">

    </div>
    {call BlocklyGames.soy.dialog /}
    <div id="help" class="dialogHiddenContent">
        <div style="padding-bottom: 0.7ex">
            {if $ij.level == 1}
                {msg meaning="Puzzle.helpText"
                        desc="Instructions for the puzzle.  For context, see [https://blockly-games.appspot.com/puzzle Blockly Puzzle]."}
                    We have to charge a battery in this region using different renewable energy resources.
                {/msg}
            {elseif $ij.level == 2}
                {msg meaning="Puzzle.helpText"
                        desc="Instructions for the puzzle.  For context, see [https://blockly-games.appspot.com/puzzle Blockly Puzzle]."}
                    We have travelled to another region and itâ€™s night time. Think of the resources we can use to charge
                    the battery here.
                {/msg}
            {elseif $ij.level == 3}
                {msg meaning="Puzzle.helpText"
                        desc="Instructions for the puzzle.  For context, see [https://blockly-games.appspot.com/puzzle Blockly Puzzle]."}
                    This looks like a beautiful green field. Think of the resources we can use to charge the battery
                    here.
                {/msg}
            {/if}
        </div>
//        <div id="sample" class="readonly"></div>
        {call BlocklyGames.soy.ok /}
    </div>
    <div id="answers" class="dialogHiddenContent">
        <div id="answerMessage">
        </div>
        <div id="graph">
            <div id="graphValue"></div>
        </div>
        {call BlocklyGames.soy.ok /}
    </div>
{/template}
